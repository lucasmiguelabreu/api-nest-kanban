{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/main.ts"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs16.x",
      "memory": 1024,
      "timeout": 10
    }
  }
}
